{
  "name": "@jimengio/jasmin-ui",
  "version": "0.1.2",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:jimengio/jasmin-ui.git",
  "author": "rebirthO <benyue.liu@jimeng.io>",
  "license": "ISC",
  "keywords": [
    "jasmin-ui",
    "ui",
    "jimengio",
    "@jimengio/jasmin-ui"
  ],
  "scripts": {
    "dev": "webpack-dev-server --hot",
    "watch": "webpack-dev-server --hot",
    "release": "webpack --config webpack.release.js",
    "remove-lib": "rm -rf lib",
    "prettier": "node ./scripts/prettier.js",
    "cp-antd-css": "mkdir -p lib/assets/antd && cp -f node_modules/antd/dist/antd.min.css lib/assets/antd/",
    "compile": "tsc -d -p tsconfig.release.json --outDir lib/",
    "prepare": "yarn remove-lib && yarn compile && yarn cp-antd-css",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:~/repo/jimengio/jasmin-ui"
  },
  "dependencies": {
    "antd": "3.13.5",
    "emotion": "10.0.7",
    "prop-types": "15.7.2",
    "react": "16.8.3"
  },
  "devDependencies": {
    "@types/node": "^11.11.6",
    "@types/react": "16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "prettier": "^1.16.4",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "style-loader": "^0.23.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.3.3333",
    "typescript-styled-plugin": "^0.14.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0"
  }
}
